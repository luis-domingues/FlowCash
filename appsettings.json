{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "FlowCash",
    "Audience": "FlowCashClients",
    "ExpirationMinutes": 60
  },
  "SendGrid": {
    "ApiKey": "${SENDGRID_API_KEY}"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=${DB_NAME};Username=${DB_USERNAME};Password=${DB_PASSWORD}"
  }
}
